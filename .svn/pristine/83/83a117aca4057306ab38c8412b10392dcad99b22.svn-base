}
.state-manage, .state-del{
    float:left;
    display:inline;
}
.state-del{margin-left:4px;}
.state-manage a{
     width:60px;
}
.state-shanchu a , .state-del a{
    color: #08c;
	display:block;
	height:20px;
	line-height:20px;
	padding-left:22px;
	background:url(images/shanchu_u.png) no-repeat left top;
    text-decoration: none;
}

