    line-height: 16px;
    padding:4px 0 0 0;
	margin:0px;
    overflow: hidden;
    height:32px;
}
.appChild .app-control{
    color: #08c;
    font-size: 12px;
    line-height: 16px;
    padding:4px 0 0 0;
    margin:0px;
    overflow: hidden;    
}
.state-guanli a , .state-manage a{
    color: #08c;
	display:block;
	height:20px;
	line-height:20px;
	width:28px;
	padding-left:20px;
	background:url(images/guanli_u.png) no-repeat left top;
