	display: inline-block;
	background: url(button.gif) no-repeat right -81px;
	height: 27px;
	line-height: 27px;
	color:#fff;
	font-size: 12px;
	font-weight: normal;
	cursor: pointer;
	text-decoration: none;
	padding-right: 14px;
	font-style: normal;
}
.btn_normal:hover em{
	color: #333;
	background-position:right -27px;
}

.btn_save{
	padding-left: 24px;
}
.btn_save em{
	padding-right: 24px;
}

em{font-style: normal;}
