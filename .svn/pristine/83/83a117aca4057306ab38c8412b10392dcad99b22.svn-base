/* list2 */
.appParent{
	overflow:hidden;
}
.appChild{
	border-color: #FFFFFF #F3F3F3 #F3F3F3 #FFFFFF;
    border-style: solid;
    border-width: 1px;
    float: left;
    padding: 7px 4px 6px 7px;
    width: 220px;
    overflow: hidden;
    position: relative;
}
.appChild .state-guanli{
    position: absolute;
    right: 7px;
    top: 6px;
	display: block;
}
.appChild .state-shanchu{
    position: absolute;
    right: 7px;
    top: 32px;
	display: block;
}
