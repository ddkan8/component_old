	padding: 5px;
}
.search_table td {
	padding: 4px 6px;
	text-align: left;
}

.btn_normal {
	display: inline-block;
	background: url(button.gif) no-repeat left -54px;
	height: 27px;
	color:#fff;
	line-height: 27px;
	font-size: 12px;
	font-weight: normal;
